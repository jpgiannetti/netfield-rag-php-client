{
    "name": "jpgiannetti/netfield-client",
    "description": "PHP client library for Netfield API - A production-ready multi-tenant document intelligence platform (RAG + DIS modules)",
    "type": "library",
    "license": "MIT",
    "keywords": ["netfield", "rag", "dis", "ai", "llm", "document", "search", "api", "client", "classification"],
    "homepage": "https://github.com/jpgiannetti/netfield-platform",
    "authors": [
        {
            "name": "Jean-Philippe Giannetti",
            "email": "jpgiannetti@users.noreply.github.com"
        }
    ],
    "support": {
        "issues": "https://github.com/jpgiannetti/netfield-platform/issues",
        "source": "https://github.com/jpgiannetti/netfield-platform"
    },
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.0",
        "firebase/php-jwt": "^6.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "symfony/translation-contracts": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Netfield\\RagClient\\": "src/",
            "Ged\\": "src/Compat/Ged/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netfield\\RagClient\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse src tests --level=7",
        "cs-check": "phpcs src tests --standard=PSR12",
        "cs-fix": "phpcbf src tests --standard=PSR12"
    }
}