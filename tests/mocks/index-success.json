{
  "request": {
    "method": "POST",
    "url": "/api/v1/index",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      },
      "Authorization": {
        "matches": "Bearer .*"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"],
    "jsonBody": {
      "status": "success",
      "message": "Document indexed successfully",
      "document_id": "test-document-id",
      "processing_time": 0.75
    }
  }
}